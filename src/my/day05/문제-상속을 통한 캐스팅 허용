public class Main {

	public static void main(String[] args) {
		
		개발자A a개발자 = new 개발자A();
		a개발자.자바프로그래밍();
		a개발자.C프로그래밍();
		a개발자.파이썬프로그래밍();
		

		개발자B b개발자 = new 개발자B();
		b개발자.C프로그래밍();
		b개발자.자바프로그래밍();
		
		
		개발자C c개발자  = new 개발자C();
		c개발자.파이썬프로그래밍();

	}

}

class 개발자A { // is -a 관계가 어색하다. 이를 컴포지션으로 개선하면
	// 파이썬 개발을 해야한다면..
	// 개발자C에게 맡긴다. 그럴려면  개발자C 객체 정보를 갖고 있어야 한다.
	
	// 컴포지션 관계(has-a 구성 관계)
	개발자C  c개발자 = new 개발자C();
	개발자B  b개발자 = new 개발자B();
	
	void 자바프로그래밍() {
		System.out.println("자바프로그래밍");
	}
	// 파이썬프로그래밍 메소드 호출 시 개발자c에게 맡긴다.
	void 파이썬프로그래밍() {
		c개발자.파이썬프로그래밍();
	}
	void C프로그래밍() {
		b개발자.C프로그래밍();
	}
}

class 개발자B {
	
	개발자A a개발자 = new 개발자A();
	void C프로그래밍() {
		System.out.println("C프로그래밍");
	}
	// 마찬가지 개발자A에게 맡긴다.
	void 자바프로그래밍() {
		a개발자.자바프로그래밍();
	}
}
class 개발자C{
	void 파이썬프로그래밍() {
		System.out.println("파이썬프로그래밍");
	}
}
